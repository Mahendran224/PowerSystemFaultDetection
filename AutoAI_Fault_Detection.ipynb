   "source": [
    "# üîê API Credentials\n",
    "from ibm_watson_machine_learning import APIClient\n",
    "\n",
    "API_KEY = 'wa0L9UzDEOWC9eofh3uJiXjaH1rGW35pJssSlsCXT3uI'\n",
    "wml_credentials = {\n",
    "    "url": "https://au-syd.ml.cloud.ibm.com",\n",
